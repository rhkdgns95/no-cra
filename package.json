  "scripts": {
    "test": "mocha",
    "test:nyc": "nyc mocha -r @babel/register -r ts-node/register -r ./test/dom.js -r ./test/helpers.js ./src/**/*.spec.*",
    "start": "parcel -p 3000 ./src/index.html --open",
    "build": "parcel build ./src/index.html",
    "test:unit": "mocha -r @babel/register -r ts-node/register -r ./test/dom.js -r ./test/helpers ./src/**/*.spec.*",
    "test:unit:watch": "npm run test:unit -- --watch",
    "prettier": "prettier --write --config ./.prettierrc \"./src/**/!(*.spec).{ts,tsx}\"",
    "lint": "eslint \"./src/**/!(*.spec).{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"./src/**/!(*.spec).{js,jsx,ts,tsx}\""
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test:unit",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write --config ./.prettierrc",
      "eslint --fix"
    ]
  }
}
